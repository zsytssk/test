{
    "tabWidth": 4,
    "singleQuote": true,
    "semi": true,
    "no-prose-wrap": true,
    "trailingComma": "all",
    "comma-dangle": [2, "always-multiline"],
    "overrides": [
        {
            "files": "*.md",
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
