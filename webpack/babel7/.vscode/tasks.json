{
  "version": "2.0.0",
  "tasks": [
    {
      "taskName": "tsc",
      "command": "tsc",
      "isBackground": true,
      "type": "shell",
      "problemMatcher": "$tsc",
      "args": [
        "-p",
        "tsconfig.json"
      ],
      "presentation": {
        "reveal": "always",
        "panel": "new",
        "echo": true
      },
      "group": "build"
    },
    {
      "taskName": "primus",
      "command": "nodemon",
      "type": "shell",
      "args": [
        "app.js"
      ],
      "options": {
        "cwd": "${workspaceRoot}/server"
      },
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "webpack-dev-server_dev",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "webpack_release",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "release_js",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "release_all",
      "problemMatcher": []
    }
  ]
}